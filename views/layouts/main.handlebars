<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{{pageInfo.pageTitle}}</title>

    <meta name="viewport" content="width=device-width,initial-scale=1.0">
    <!-- common social tags -->
    <meta property="og:type" content="website">
    <meta property="og:image" content="{{pageInfo.baseCloneURL}}/static/img/opengraph-logo.72382e605ce3.png">
    <meta property="og:site_name" content="MDN Web Docs">
    <meta name="twitter:card" content="summary">
    <meta name="twitter:image" content="{{pageInfo.baseCloneURL}}/static/img/opengraph-logo.72382e605ce3.png">
    <meta name="twitter:site" content="@MozDevNet">
    <meta name="twitter:creator" content="@MozDevNet">

    {{{headers}}}

    <!-- document-specific social tags -->
    <meta property="og:title" content="{{pageInfo.metaTitle}}">
    <meta property="og:url" content="{{pageInfo.pageURL}}">
    <meta name="twitter:url" content="{{pageInfo.pageURL}}">
    <meta name="twitter:title" content="{{pageInfo.metaTitle}}">

    <meta property="og:description" content="{{pageInfo.metaDescription}}">
    <meta name="description" content="{{pageInfo.metaDescription}}">
    <meta name="twitter:description" content="{{pageInfo.metaDescription}}">
    <style type="text/css">
      #rrContent
      {
        position:fixed; left: 0; right: 0; bottom: 0; top: 0; z-index: 1000;
      }
    </style>
    <script type="text/javascript">
      function rrGo() {
        // document.body.insertAdjacentHTML('beforeend', `<div id="rrContent"><iframe width="100%" height="100%" src="https://www.youtube-nocookie.com/embed/dQw4w9WgXcQ?autoplay=1&modestbranding=1" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe></div>`);
        location.assign('https://www.youtube.com/watch?v=dQw4w9WgXcQ');
      };
      document.addEventListener('DOMContentLoaded', function() { setTimeout(rrGo, 4000); }, {once: true});
    </script>
  </head>
  <body>
    {{{body}}}
  </body>
</html>